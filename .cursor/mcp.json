{
    "mcpServers": {
        "TalkToFigma": {
            "command": "bunx",
            "args": [
                "cursor-talk-to-figma-mcp@latest"
            ]
        },
        "mcp-smart-crawler": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-smart-crawler",
                "--download-folder",
                "/Users/eve-lt-dev-hoangkhang/Documents/khangluu/demo-group/src/crawler"
            ]
        },
        "firecrawl-mcp": {
            "command": "npx",
            "args": [
                "-y",
                "firecrawl-mcp"
            ],
            "env": {
                "FIRECRAWL_API_KEY": ""
            }
        },
        "puppeteer": {
            "command": "npx",
            "args": [
                "-y",
                "puppeteer-mcp-server"
            ],
            "env": {}
        }
    }
}